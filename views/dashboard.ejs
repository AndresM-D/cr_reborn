<body>
<div id="Dashboard">
    <img id="imgBg" src="/img/dashboard/imgBg.png">

    </svg>
    <svg class="imgShadow_m">
        <linearGradient id="imgShadow_m" spreadMethod="pad" x1="0.5" x2="0.5" y1="0" y2="1">
            <stop offset="0" stop-color="#000" stop-opacity="0.678"></stop>
            <stop offset="1" stop-color="#000" stop-opacity="0.612"></stop>
        </linearGradient>
        <rect id="imgShadow_m" rx="0" ry="0" x="0" y="0" width="1920" height="1080">
        </rect>
    </svg>
    <svg class="Line_5" viewBox="0 0 1589.5 1">
        <path id="Line_5" d="M 0 0 L 1589.5 0">
        </path>
    </svg>
    <div id="dashOptions">
        <div id="Dashboard_p">
            <span>Dashboard</span>
        </div>
        <div id="User">
            <span><%= name %></span>
        </div>
        <div id="Logout">
            <span><a href="/logout" class="btn btn-secondary">Logout</a></span>
        </div>
    </div>
    <img id="Logo" src="/img/dashboard/Logo.png">

    <!-- Card -->
    <div class="card" id="card1">

        <!-- Card image -->
        <div class="view overlay">
            <img class="card-img-top" src="/img/dashboard/imgCard1.png"
                 alt="Card image cap">
            <a href="#!">
                <div class="mask rgba-white-slight"></div>
            </a>
        </div>

        <!-- Card content -->
        <div class="card-body">

            <!-- Title -->
            <h4 class="card-title">Card title</h4>
            <!-- Text -->
            <p class="card-text">Some quick example text to build on the card title and make up the bulk of the card's
                content.</p>
            <!-- Button -->
            <a href="#" class="btn btn-primary">Button</a>

        </div>

    </div>
